{
    "ver": "0.0.1",
    "authors": [
        "dylan"
    ],
    "contact": "help@kbase.us",
    "visible": true,
    "categories": ["active","sequence","comparative_genomics"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [ 
        {
            "id": "desc",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
	    "text_options": {
	        "is_output_name": true
	    }
        },
        {
            "id": "input_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.MSA" ]
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.MSA" ],
		"is_output_name": true		  
            }
        },
        {
            "id": "trim_level",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        },
        {
            "id": "input_mask_name",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.MSA" ]
            }
        }

    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_gblocks",
            "method": "run_Gblocks",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "desc",
                    "target_property": "desc"
                },
                {
                    "input_parameter": "input_name",
                    "target_property": "input_name"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },
                {
                    "input_parameter": "trim_level",
                    "target_property": "trim_level"
                },
                {
                    "input_parameter": "input_mask_name",
                    "target_property": "input_mask_name"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "service_method_output_path": [0, "output_ref"],
                    "target_property": "output_ref"
                },
                {
		    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
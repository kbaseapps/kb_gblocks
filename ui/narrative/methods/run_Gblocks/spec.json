{
    "ver": "0.0.1",
    "authors": [
        "dylan"
    ],
    "contact": "help@kbase.us",
    "visible": true,
    "categories": ["active","sequence","comparative_genomics"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "desc",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
	    "text_options": {
	        "is_output_name": false
	    }
        },
        {
            "id": "input_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.MSA" ]
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseTrees.MSA" ],
		"is_output_name": true		  
            }
        },
        {
            "id": "trim_level",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0",
		"max_integer": "2"
            }
        },
        {
            "id": "min_seqs_for_conserved",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0"
            }
        },
        {
            "id": "min_seqs_for_flank",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0"
            }
        },
        {
            "id": "max_pos_contig_nonconserved",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "8" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "0"
            }
        },
        {
            "id": "min_block_len",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "10" ],
            "field_type": "text",
            "text_options": {
		"validate_as": "int",
		"min_integer": "2"
            }
        },
        {
            "id": "remove_mask_positions_flag",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "checkbox",
            "checkbox_options": {
		"checked_value": "1",
		"unchecked_value": "0"
            }
        }

    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_gblocks",
            "method": "run_Gblocks",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "desc",
                    "target_property": "desc"
                },
                {
                    "input_parameter": "input_ref",
                    "target_property": "input_ref",
		    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },
                {
                    "input_parameter": "trim_level",
                    "target_property": "trim_level"
                },
                {
                    "input_parameter": "min_seqs_for_conserved",
                    "target_property": "min_seqs_for_conserved"
                },
                {
                    "input_parameter": "min_seqs_for_flank",
                    "target_property": "min_seqs_for_flank"
                },
                {
                    "input_parameter": "max_pos_contig_nonconserved",
                    "target_property": "max_pos_contig_nonconserved"
                },
                {
                    "input_parameter": "min_block_len",
                    "target_property": "min_block_len"
                },
                {
                    "input_parameter": "remove_mask_positions_flag",
                    "target_property": "remove_mask_positions_flag"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
		    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}